{
  "name": "suave-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3000 --host 0.0.0.0",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:spelling",
    "lint:ci": "npm lint:js --quiet && npm run lint:style && npm run lint:spelling",
    "lint:js": "eslint --cache --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx,mjs}\"",
    "lint:spelling": "cspell \"**\" --no-progress",
    "lint:style": "stylelint \"src/css/custom.css\"",
    "format": "prettier --write .",
    "format:diff": "prettier --list-different ."
  },
  "dependencies": {
    "@docusaurus/core": "^3.1.1",
    "@docusaurus/plugin-content-docs": "^3.1.1",
    "@docusaurus/plugin-sitemap": "^3.1.1",
