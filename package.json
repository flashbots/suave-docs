    "@docusaurus/theme-mermaid": "^3.1.1",
    "@docusaurus/utils": "^3.1.1",
    "@mdx-js/react": "^3.0.0",
    "@metamask/sdk-react": "^0.9.0",
    "@vercel/analytics": "^0.1.11",
    "autoprefixer": "^10.4.16",
    "axios": "^1.4.0",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "prism-react-renderer": "^2.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-katex": "^6.0.3",
    "remark-math": "^5.1.1",
    "tailwindcss": "^3.3.3"
